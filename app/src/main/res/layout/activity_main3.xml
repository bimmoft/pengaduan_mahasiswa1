<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textViewNotif"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Notif"
        android:textColor="#3470A2"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="162dp"
        android:layout_marginTop="128dp" />

    <TextView
        android:id="@+id/textViewIkasi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ikasi"
        android:textColor="#FFC727"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/textViewNotif"
        app:layout_constraintTop_toTopOf="@+id/textViewNotif"
        android:layout_marginStart="0dp" />


    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/linenotif"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewNotif"
        android:layout_marginStart="11dp"
        android:layout_marginTop="16dp" />


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="48dp"
        android:layout_height="39dp"
        app:srcCompat="@drawable/baseline_lock_24"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        android:layout_marginStart="31dp"
        android:layout_marginTop="-50dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="75dp"
        android:layout_height="60dp"
        app:srcCompat="@drawable/circlenotif"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        android:layout_marginStart="16dp"
        android:layout_marginTop="15dp" />


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Keamanan"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        android:layout_marginStart="98dp"
        android:layout_marginTop="-70dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="302dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="88dp"
        android:layout_marginTop="200dp"
        android:contentDescription="Status pengaduan belum dikonfirmasi. Cek laman pelacakan untuk pembaruan."
        android:padding="10dp"
        android:text="Pengaduan anda mengenai keamanan belum dikonfirmasi. Silahkan cek secara berkala pada laman pelacakan."
        android:textColor="#000000"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/linenotif"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        android:layout_marginStart="11dp"
        android:layout_marginTop="1dp" />


    <ImageView
        android:id="@+id/back_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_back_arrow"
        android:contentDescription="@string/back"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>